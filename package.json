{
  "name": "tic-tac-toe-multiplayer",
  "version": "1.0.0",
  "description": "Real-time multiplayer Tic Tac Toe game with Node.js and Socket.IO",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
     "test-soft": "jest || exit 0",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "docker:build": "docker build -t tic-tac-toe .",
    "docker:run": "docker run -p 3000:3000 tic-tac-toe",
    "docker:push": "docker push your-dockerhub-username/tic-tac-toe"
  },
  "keywords": [
    "tic-tac-toe",
    "multiplayer",
    "nodejs",
    "socket.io",
    "game"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "socket.io-client": "^4.7.4",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}